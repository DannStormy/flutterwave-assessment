  "description": "",
  "scripts": {
    "dev": "nodemon --exec babel-node index.js",
    "prestart": "npm run migrate:up",
    "start": "babel-node index.js",
    "migrations": "source .env && db-migrate",
    "lint": "eslint --fix app/ --ext .js",
    "precommit": "npm run lint",
    "migrate:up": "db-migrate up  --config database.json",
    "migrate:reset": "db-migrate reset --config database.json",
    "seed:create": "db-migrate create:seed --config database.json",
    "seed:up": "db-migrate up:seed --config database.json -e test",
    "seed:down": "db-migrate down:seed --config database.json -e test",
    "seed:reset": "db-migrate reset:seed --config database.json -e test"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@sendgrid/mail": "^7.7.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.15.2",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "debug": "^2.6.0",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "express": "^4.14.0",
    "express-async-errors": "^3.1.1",
    "file-stream-rotator": "0.0.7",
    "helmet": "^3.23.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.1",
    "joi-password": "^4.1.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.7.0",
    "pg-copy-streams": "^6.0.6",
    "pg-copy-streams-binary": "^2.2.0",
    "pg-promise": "^11.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "babel-node": "^0.0.1-security",
    "nodemon": "^3.0.1"
  }
}
